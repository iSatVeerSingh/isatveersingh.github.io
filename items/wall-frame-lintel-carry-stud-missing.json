{
  "category": "Wall Frame",
  "itemName": "Lintel Carry Stud Missing",
  "children": [
    {
      "type": "PARAGRAPH",
      "children": [
        {
          "text": "There has not been jamb carry studs installed to the ends of the lintels over all openings.",
          "bold": false,
          "italics": false
        }
      ]
    },
    {
      "type": "ITEM_IMAGES_NOTES"
    },
    {
      "type": "PARAGRAPH",
      "children": [
        {
          "text": "",
          "bold": false,
          "italics": false
        }
      ]
    },
    {
      "type": "PARAGRAPH",
      "children": [
        {
          "text": "It is a requirement of AS 1684 Residential Timber-Framed Construction, clause 6.3.2.3 Jamb Studs (studs at sides of openings), that",
          "bold": false,
          "italics": false
        },
        {
          "text": "‘openings are to have 1 full-length stud on either side of the openings’.",
          "bold": true,
          "italics": true
        }
      ]
    },
    {
      "type": "PARAGRAPH",
      "children": [
        {
          "text": "",
          "bold": false,
          "italics": false
        }
      ]
    },
    {
      "type": "PARAGRAPH",
      "children": [
        {
          "text": "Additional jamb studs will have to be installed to the required ends of these lintels.",
          "bold": false,
          "italics": false
        }
      ]
    }
  ]
}